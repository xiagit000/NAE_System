	background: url("../images/liDot.png") no-repeat scroll 0 8px transparent;
    color: #004176;
    font-size: 14px;
    line-height:25px;
    padding-left: 10px;
	width:370px;
	overflow: hidden;
   text-overflow: ellipsis;
   white-space:nowrap;
   display:inline-block;
   padding-right:10px;
	}
.table_list a{
	width:800px;
